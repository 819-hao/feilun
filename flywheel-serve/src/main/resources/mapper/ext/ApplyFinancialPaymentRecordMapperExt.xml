<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seeease.flywheel.serve.financial.mapper.ApplyFinancialPaymentRecordMapper">

    <select id="getPage"
            resultType="com.seeease.flywheel.financial.result.ApplyFinancialPaymentRecordPageResult">
        select *
        from apply_financial_payment_record
        <where>
            deleted = 0
            <if test="request.applyFinancialPaymentId != null">
                and apply_financial_payment_id = #{request.applyFinancialPaymentId}
            </if>
        </where>
        order by id desc
    </select>
</mapper>
