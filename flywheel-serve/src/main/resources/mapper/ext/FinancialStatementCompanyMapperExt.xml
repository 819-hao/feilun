<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seeease.flywheel.serve.maindata.mapper.FinancialStatementCompanyMapper">
    <select id="queryCompanyName"
            resultType="com.seeease.flywheel.serve.maindata.entity.FinancialStatementCompany">
        SELECT *
        FROM financial_statement_company
        WHERE find_in_set(#{subject}, subject_id)
    </select>
</mapper>
