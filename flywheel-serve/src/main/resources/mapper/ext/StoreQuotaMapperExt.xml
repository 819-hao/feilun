<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seeease.flywheel.serve.maindata.mapper.StoreQuotaMapper">

    <select id="selectQuota" resultType="java.math.BigDecimal">
        select quota from store_quota where sm_id = #{smId} and deleted = 0 and #{now} between `start` and `end` limit 1
    </select>
</mapper>
