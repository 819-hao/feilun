<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seeease.flywheel.serve.allocate.mapper.BillAllocateLineMapper">


    <select id="selectStateByAllocateId" resultType="java.lang.Integer">
        select allocate_line_state
        from bill_allocate_line
        where allocate_id = #{allocateId}
    </select>
</mapper>
