<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seeease.flywheel.serve.goods.mapper.BuyBackPolicyActivityMapper">

    <select id="selectByStockId" resultType="com.seeease.flywheel.serve.goods.entity.BuyBackPolicyActivity">
        select *
        from buy_back_policy_activity
        where stock_id = #{stockId}
          and activity_type = #{activityType}
    </select>
</mapper>
