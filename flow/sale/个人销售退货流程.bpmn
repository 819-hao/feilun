        <camunda:in local="true" variables="all" />
        <camunda:in sourceExpression="#{execution.processBusinessKey}" target="parentBusinessKey" />
        <camunda:in sourceExpression="#{shortcodes}" target="shortcodes" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0dufwnn</bpmn:incoming>
      <bpmn:outgoing>Flow_09bl5i9</bpmn:outgoing>
