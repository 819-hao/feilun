          <camunda:formField id="businessKey" label="采购单号" type="string" />
        </camunda:formData>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="Groovy">   String shop = execution.getVariable("shortcodes")
    if(null != shop){
        execution.setVariable("authGroups", shop+"shopowner" +","+shop+"shopclerk")
    }else {
        execution.setVariable("authGroups", "buy")
    }</camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_19d4cnt</bpmn:outgoing>
    </bpmn:startEvent>
